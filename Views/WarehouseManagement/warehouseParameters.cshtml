
@{
    ViewBag.Title = "warehouseParameters";
}


@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<WarehouseLevel>


<section class="content-header">
   

    <div class="box-body">
        <a class="btn btn-app btn btn-primary" href="~/warehouseManagement/newWarehouseParameter">
            <i class="fa fa-file"></i> Yeni
        </a>
        <a class="btn btn-app">
            <i class="fa fa-share-square" data-toggle="modal" data-target="#ModalButton"></i> Aktarım
        </a>
        <div class="modal fade" id="ModalButton" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h3>Dışa Aktar</h3>
                    </div>
                    <div class="modal-body">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
                            <label class="form-check-label" for="inlineRadio1">Text File (.txt)</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
                            <label class="form-check-label" for="inlineRadio2">Excel File (.xlsx)</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3">
                            <label class="form-check-label" for="inlineRadio1">Html File (.txt)</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio4" value="option4">
                            <label class="form-check-label" for="inlineRadio2">PDF File (.pdf)</label>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Aktar</button>
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Kapat</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3>
                        Depo Parametreleri

                    </h3>

                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Malzeme Kodu Ve Tanımı</th>
                                <th>Depo Adı</th>
                                <th>Asgari Seviye</th>
                                <th>Azami Seviye</th>
                                <th>Güvenli Seviye</th>
                                <th>Giriş Kontrol</th>
                                <th>Çıkış Kontrol</th>
                                <th>Asgari Seviye Kontrol</th>
                                <th>Azami Seviye Kontrol</th>
                                <th>Güvenli Seviye Kontrol</th>
                                <th>Nagatif Seviye Kontrol</th>
                                <th>Depo Yöntemi</th>
                                <th>Depo Yöntemi Kontrolü</th>
                                <th>İşlem</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (WarehouseLevel wareHouseParameter in Model)
                            {

                            <tr>
                                <td>@wareHouseParameter.Material</td>
                                <td>@wareHouseParameter.Warehouse</td>
                                <td>@wareHouseParameter.MinimumLevel</td>
                                <td>@wareHouseParameter.MaximumLevel</td>
                                <td>@wareHouseParameter.TrustedLevel</td>
                                <td>@wareHouseParameter.InputControl</td>
                                <td>@wareHouseParameter.OutputControl</td>
                                <td>@wareHouseParameter.MinimumControl</td>
                                <td>@wareHouseParameter.MaxiumumControl</td>
                                <td>@wareHouseParameter.TrustedControl</td>
                                <td>@wareHouseParameter.NegativeLevelControl</td>
                                <td>@wareHouseParameter.InventoryMethod</td>
                                <td>@wareHouseParameter.InventoryMethod</td>
                                <td>
                                    <div class="dropdown btn-group dropleft">
                                        <button class="btn  btn-primary dropdown-toggle " type="button" id="dropdownMenuButton" data-toggle="dropdown">
                                            Seç
                                        </button>
                                        <div class="dropdown-menu">
                                            <a href="@Url.Action("detailWarehouseParameter", "warehouseManagement", new { id = wareHouseParameter.ID })"><span class="fa fa-eye"> Görüntüle </span></a> <br />
                                            <a href="@Url.Action("editWarehouseParameter", "warehouseManagement", new { id = wareHouseParameter.ID })"><span class="fa fa-edit"> Düzenle </span></a> <br />
                                            <a href="@Url.Action("deleteWarehouseParameter", "warehouseManagement", new { id = wareHouseParameter.ID })"><span class="fa fa-trash"> Sil </span></a> <br />


                                        </div>

                                    </div>
                                </td>
                            </tr>

                            }
                        </tbody>
                       
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>







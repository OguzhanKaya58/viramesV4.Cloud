
@{
    ViewBag.Title = "ImproprietyStepDefinitions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@{
    ViewBag.Title = "unitSetDefinition";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<NonConformanceStep>


<section class="content-header">
    <h1>
        Uygunsuzluk Adım Tanımları

    </h1>

    <div class="box-body ">
        <div class="btn-group">


            <button type="button" class="btn btn-primary ">
                <span class=" fa fa-file"></span> Yeni
            </button>
        </div>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-edit"></span> Düzenle
        </button>

        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>

        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-copy"></span> Kopyala
        </button>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
        <button type="button" class="btn btn-danger">
            <span class=" fa fa-trash"></span> Sil
        </button>
    </div>


    <ol class="breadcrumb">
        <li class="active">Uygunsuzluk Adım Tanımları</li>
    </ol>



</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Kod</th>
                                <th>Tanım</th>
                                <th>Hareket Tipi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (NonConformanceStep impropriety in Model)
                            {

                                <tr>
                                    <td>@impropriety.Code</td>
                                    <td>@impropriety.Definition</td>
                                    <td>@impropriety.StepType</td>
                                    
                                

                            </tr>

                        }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Kod</th>
                                <th>Tanım</th>
                                <th>Hareket Tipi</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>



@{
    ViewBag.Title = "qualityControlCriteriaSet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<CriteriaSet>


<section class="content-header">
    <h1>
        Kriter Seti Tanımları

    </h1>

    <div class="box-body ">
        <div class="btn-group">


            <button type="button" class="btn btn-primary ">
                <span class=" fa fa-file"></span> Yeni
            </button>

        </div>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-edit"></span> Düzenle
        </button>

        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>

        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-print"></span> Kopyala
        </button>
        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-id-card"></span> Kullanıldığı Kartlar
        </button>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
        <button type="button" class="btn btn-danger">
            <span class=" fa fa-trash"></span> Sil
        </button>
    </div>


    <ol class="breadcrumb">
        <li class="active">Kriter Seti Tanımları</li>
    </ol>



</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Kriter Seti Kodu</th>
                                <th>Kriter Seti Tanımı</th>
                                <th>Özel Kod</th>
                                <th>Durum</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (CriteriaSet qualityControl in Model)
                            {

                                <tr>
                                    <td>@qualityControl.Code</td>
                                    <td>@qualityControl.Definition</td>
                                    <td>@qualityControl.SpeCode</td>
                                    <td>@qualityControl.Status</td>
                                </tr>

                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Kriter Seti Kodu</th>
                                <th>Kriter Seti Tanımı</th>
                                <th>Özel Kod</th>
                                <th>Durum</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>






@{
    ViewBag.Title = "purchaseReceiptsApprovalTransactions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@{
    ViewBag.Title = "correctivePreventiveActivities";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<MaterialFiche>


<section class="content-header">
    <h1>
        Satın Alma İrsaliyeleri Kalite Kontrol Onay Hareketleri

    </h1>

    <div class="box-body ">
        <div class="btn-group">


            <button type="button" class="btn btn-primary ">
                <span class=" fa fa-clipboard"></span> Kalite Kontrol Sonuç Girişi
            </button>

        </div>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-bar-chart"></span> Analiz Et
        </button>

        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>

        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-history"></span> Geçmiş Kalite Kontrol Kaydı
        </button>
        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-sitemap"></span> Döküman Yönetimi
        </button>
        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
       
    </div>


    <ol class="breadcrumb">
        <li class="active"> Satın Alma İrsaliyeleri Kalite Kontrol Onay Hareketleri</li>
    </ol>



</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Numarası</th>
                                <th>Tarih</th>
                                <th>Satın Alma Siparişi</th>
                                <th>Cari Hesap</th>
                                <th>Proje</th>
                                <th>Depo</th>
                                <th>Personel</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MaterialFiche purchaseReceipts in Model)
                            {

                                <tr>
                                    <td>@purchaseReceipts.Number</td>
                                    <td>@purchaseReceipts.Date</td>
                                    <td>@purchaseReceipts.PurchaseOrder</td>
                                    <td>@purchaseReceipts.Account</td>
                                    <td>@purchaseReceipts.Project</td>
                                    <td>@purchaseReceipts.Warehouse</td>
                                    <td>@purchaseReceipts.Employee</td>
                                   
                                </tr>

                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Numarası</th>
                                <th>Tarih</th>
                                <th>Satın Alma Siparişi</th>
                                <th>Cari Hesap</th>
                                <th>Proje</th>
                                <th>Depo</th>
                                <th>Personel</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>








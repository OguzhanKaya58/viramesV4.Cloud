@using viramesV4.Object.BusinessLogicLayer.System
@model IList<LockedObject>
@{
    ViewBag.Title = "lockedObjecr";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        Kilitlenmiş Kayıtlar
    </h1>
    <div class="box-body ">
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-unlock"></span> Kilidi Aç
        </button>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
    </div>


</section>

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Kullanıcı</th>
                                <th>Modül</th>
                                <th>Çalışma Alanı</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (LockedObject lockedObject in Model)
                            {
                                <tr>
                                    <td>@lockedObject.User</td>
                                    <td>@lockedObject.Module</td>
                                    <td>@lockedObject.Workarea</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>


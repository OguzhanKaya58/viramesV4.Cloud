@using viramesV4.Object.BusinessLogicLayer.System
@model IList<Log>
@{
    ViewBag.Title = "Log";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        Log Kayıtları
    </h1>
    <div class="box-body ">
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
    </div>


</section>

<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Modül</th>
                                <th>Kullanıcı Adı</th>
                                <th>Log Tarihi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Log log in Model)
                            {
                                <tr>
                                    <td>@log.Module</td>
                                    <td>@log.UserName</td>
                                    <td>@log.LogDate</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>


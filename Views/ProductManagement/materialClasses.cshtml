
@{
    ViewBag.Title = "materialClasses";
}


@{
    ViewBag.Title = "purchaseFiche";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<MaterialClass>


<section class="content-header">
    

    <div class="box-body">
        <a class="btn btn-app btn btn-primary">
            <i class="fa fa-file"></i> Yeni
        </a>
        <a class="btn btn-app">
            <i class="fa fa-share-square"></i> Aktarım
        </a>
    </div>

</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h2>
                        Malzeme Sınıfları

                    </h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Malzeme Sınıfı Kodu</th>
                                <th>Malzeme Sınıfı Tanımı</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MaterialClass materialClasses in Model)
                            {

                                <tr>
                                    <td>@materialClasses.Code</td>
                                    <td>@materialClasses.Definition</td>
                                    
                                </tr>

                            }
                        </tbody>
                      
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>


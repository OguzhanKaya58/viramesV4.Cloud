
@{
    ViewBag.Title = "detailDocumentRecord";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<Material>

<section class="content-header">

    <div class="box-body ">
        <a class="btn btn-app btn btn-primary" onclick="goBack()">
            <i class="fa fa-arrow-left"></i> Geri
        </a>
    </div>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h2>
                        Dökümana Atanan Kayıtlar

                    </h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Malzeme Kodu ve Tanımı</th>
                                <th>Malzeme Türü</th>
                                <th>Birim Seti</th>
                                <th>Raf Konumu</th>
                                <th>Varsayılan Depo</th>
                                <th>İzleme Türü</th>
                                <th>Malzeme Durumu</th>
                                <th>Fiyatı</th>
                                <th>Miktarı</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Material material in Model)
                            {

                                <tr>
                                    <td>@material.CodeAndDefinition</td>
                                    <td>@material.MaterialType</td>
                                    <td>@material.UnitSet</td>
                                    <td>@material.ShelfLocation</td>
                                    <td>@material.DefaultWarehouse</td>
                                    <td>@material.TraceType</td>
                                    <td>@material.Status</td>
                                    <td>@material.Price</td>
                                    <td>@material.OnHand</td>


                                </tr>

                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>







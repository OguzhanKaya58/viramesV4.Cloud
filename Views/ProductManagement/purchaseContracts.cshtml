
@{
    ViewBag.Title = "purchaseContracts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<PurchaseContract>

<section class="content-header">

    <div class="box-body">
        <a class="btn btn-app btn btn-primary" href="~/ProductManagement/materialDefinitions"> <span class=" fa fa-backward"></span>Geri</a>

    </div>

</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h2>
                        Dökümana Atanan Kayıtlar

                    </h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <h2>Satınalma Sözleşmeleri</h2>
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Numara</th>
                                <th>Başlama Tarihi</th>
                                <th>Bitiş Tarihi</th>
                                <th>Cari Hesap</th>
                                <th>Proje</th>
                                <th>Görünürlülük</th>
                                <th>Teklif</th>
                                <th>Durum</th>
                                <th>Toplam Masraf</th>
                                <th>Toplam Masraf (KDV Dahil)</th>
                                <th>Toplam İskonto</th>
                                <th>Ürün Fiyatı Toplamı</th>
                                <th>Toplam Malzeme Fiyatı (KDV Dahil)</th>
                                <th>Toplam </th>
                                <th>Toplam (KDV Dahil)</th>
                                <th>Toplam KDV</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (PurchaseContract material in Model)
                            {

                                <tr>
                                    <td>@material.Number</td>
                                    <td>@material.StartDate</td>
                                    <td>@material.EndDate</td>
                                    <td>@material.Account</td>
                                    <td>@material.Project</td>
                                    <td>@material.Visibility</td>
                                    <td>@material.Proposal</td>
                                    <td>@material.ApprovalInformation</td>
                                    <td>@material.Proposal.TotalExpense</td>
                                    <td>@material.Proposal.TotalExpenseWithVAT</td>
                                    <td>@material.Proposal.TotalDiscount</td>
                                    <td>@material.Proposal.TotalMaterialPrice</td>
                                    <td>@material.Proposal.TotalMaterialPriceWithVAT</td>
                                    <td>@material.Proposal.Balance</td>
                                    <td>@material.Proposal.BalanceWithVAT</td>
                                    <td>@material.Proposal.TotalVAT</td>


                                </tr>

                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>



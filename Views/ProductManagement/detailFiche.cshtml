
@{
    ViewBag.Title = "detailFiche";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<MaterialFiche>

<section class="content-header">

    <div class="box-body ">
        <a class="btn btn-app btn btn-primary" onclick="goBack()">
            <i class="fa fa-arrow-left"></i> Geri
        </a>
    </div>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h2>
                        Dökümana Atanan Kayıtlar

                    </h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <h2> Malzeme Fişleri </h2>
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>İrsaliye Numarası</th>
                                <th>Türü</th>
                                <th>Numarası</th>
                                <th>Satınalma Siparişi</th>
                                <th>Depo</th>
                                <th>Depo Girdi</th>
                                <th>Proje</th>
                                <th>İş Emri</th>
                                <th>Personel</th>
                                <th>Tarih</th>
                                <th>Malzeme Fiş Durumu</th>
                                <th>Onay Durumu</th>
                                <th>Bilgi Notu</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MaterialFiche material in Model)
                            {

                                <tr>
                                    <td>@material.DispatchNumber</td>
                                    <td>@material.Type</td>
                                    <td>@material.Number</td>
                                    <td>@material.PurchaseOrder</td>
                                    <td>@material.Warehouse</td>
                                    <td>@material.InputWarehouse</td>
                                    <td>@material.Workorder</td>
                                    <td>@material.Employee</td>
                                    <td>@material.Date</td>
                                    <td>@material.MaterialFicheStatus</td>
                                    <td>@material.FicheApprove</td>
                                    <td>@material.InformationNote</td>


                                </tr>

                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>

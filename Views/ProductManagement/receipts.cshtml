
@{
    ViewBag.Title = "receipts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<BOM>

<section class="content-header">

    <div class="box-body ">
        <a class="btn btn-app btn btn-primary" onclick="goBack()">
            <i class="fa fa-arrow-left"></i> Geri
        </a>
    </div>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h2>
                        Dökümana Atanan Kayıtlar

                    </h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <h2> Reçeteler </h2>
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Reçete Kodu ve Tanımı</th>
                                <th>Malzeme</th>
                                <th>Reçete Kaynağı</th>
                                <th>Depo</th>
                                <th>İş İstasyonu Havuzu</th>
                                <th>İş İstasyonu</th>
                                <th>Öncelik</th>
                                <th>Miktar</th>
                                <th>Türü</th>
                                <th>Durum</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (BOM material in Model)
                            {

                                <tr>
                                    <td>@material.CodeAndDefinition</td>
                                    <td>@material.Material</td>
                                    <td>@material.BOMSourceType</td>
                                    <td>@material.Warehouse</td>
                                    <td>@material.WorkstationGroup</td>
                                    <td>@material.Workstation</td>
                                    <td>@material.Priority</td>
                                    <td>@material.Amount</td>
                                    <td>@material.Type</td>
                                    <td>@material.Status</td>


                                </tr>

                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>









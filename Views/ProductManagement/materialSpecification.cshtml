
@{
    ViewBag.Title = "materialSpecification";
}

@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<MaterialSpecification>


<section class="content-header">
    <div class="box-body">
        
        <a class="btn btn-app btn btn-primary">
            <i class="fa fa-file"></i> Yeni
        </a>
        <a class="btn btn-app">
            <i class="fa fa-share-square"></i>Aktarım
        </a>
      

    </div>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">

                    
                    <h2  >Malzeme Özellik Tanımları</h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Malzeme Özellik Kodu</th>
                                <th>Malzeme Özellik Tanımı</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MaterialSpecification materialSpecification in Model)
                            {

                                <tr>
                                    <td>@materialSpecification.Code</td>
                                    <td>@materialSpecification.Definition</td>
                                </tr>

                            }
                        </tbody>
                     
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>


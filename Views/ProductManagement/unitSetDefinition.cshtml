
@{
    ViewBag.Title = "unitSetDefinition";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<UnitSet>


    <section class="content-header">
        <div class="box-body">
            <a class="btn btn-app btn btn-primary">
                <i class="fa fa-file"></i> Yeni
            </a>
            <a class="btn btn-app">
                <i class="fa fa-share-square"></i> Aktarım
            </a>
        </div>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">

            <div class="col-xs-12">
                <div class="box">
                    <div class="box-header">
                        <h2>
                            Birim Seti Tanımları

                        </h2>

                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Birim Seti Kodu</th>
                                    <th>Birim Seti Tanımı</th>
                                    <th>Birim Seti Durumu</th>
                                    <th>Varsayılan Birim Seti</th>
                                    <th>Özel Kod</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (UnitSet unitSetDefinition in Model)
                                {

                                    <tr>
                                        <td>@unitSetDefinition.Code</td>
                                        <td>@unitSetDefinition.Definition</td>
                                        <td>@unitSetDefinition.Status</td>
                                        <td>@if (@unitSetDefinition.Default == true) {
                                            <input type="checkbox" value="" checked>
                                             }
                                             @if (@unitSetDefinition.Default == false) {
                                            <input type="checkbox" value="" >
                                             }
                                        </td>
                                        <td>@unitSetDefinition.SpeCode</td>
                                       
                                    </tr>

                                }
                            </tbody>
                           
                        </table>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>

@{
    ViewBag.Title = "serialLotNumberMaintenanceRepairRecord";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<SerialLotNumberMaintenanceRepairRecord>


<section class="content-header">
    <div class="box-body">
        <a class="btn btn-app btn btn-primary">
            <i class="fa fa-file"></i> Yeni
        </a>
        <a class="btn btn-app">
            <i class="fa fa-share-square"></i> Aktarım
        </a>
    </div>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h2>Seri/Lot Numarası Bakım Onarım Kayıtları</h2>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Numarası</th>
                                <th>Açıklama</th>
                                <th>Bakım/Onarım Gören Malzeme</th>
                                <th>Bakım/Onarım Gören Seri/Lot Numarası</th>
                                <th>Çıkarılan(Değişen) Seri/Lot Numarası</th>
                                <th>Depo</th>
                                <th>Eklenen(Yeni) Seri/Lot Numarası</th>
                                <th>Tarih</th>
                                <th>Personel</th>
                                <th>Miktar</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (SerialLotNumberMaintenanceRepairRecord serialLotNumberMaintenanceRepairRecord in Model)
                            {

                                <tr>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Number</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Definition</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.MaintenanceMadeMaterial</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.MaintenanceMadeSerialLotNumber</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.OldSerialLotNumber</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Warehouse</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.NewSerialLotNumber</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Date</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Employee</td>
                                    <td>@serialLotNumberMaintenanceRepairRecord.Amount</td>
                                </tr>

                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>








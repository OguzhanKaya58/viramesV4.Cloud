
@{
    ViewBag.Title = "materialRequirementPlan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<MRPRecord>


<section class="content-header">
    <h1>
        Malzeme ihtiyaç Planlamaları

    </h1>

    <div class="box-body ">
        <div class="btn-group">


            <button type="button" class="btn btn-primary ">
                <span class=" fa fa-file"></span> Yeni
            </button>

        </div>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-edit"></span> Düzenle
        </button>

        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>

        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-print"></span> Rapor
        </button>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
        <button type="button" class="btn btn-danger">
            <span class=" fa fa-trash"></span> Sil
        </button>
    </div>


    <ol class="breadcrumb">
        <li class="active">Malzeme ihtiyaç Planlamaları</li>
    </ol>



</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Numara</th>
                                <th>Tarih</th>
                                <th>Depo</th>
                                <th>Üretim Emri</th>
                                <th>Talep Oluşturuldu Mu?</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (MRPRecord mRPRecord in Model)
                            {

                            <tr>
                                <td>@mRPRecord.Number</td>
                                <td>@mRPRecord.Date</td>
                                <td>@mRPRecord.Warehouse</td>
                                <td>@mRPRecord.ProductOrder</td>
                                <td>@mRPRecord.Demanded</td>
                            </tr>

                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Numara</th>
                                <th>Tarih</th>
                                <th>Depo</th>
                                <th>Üretim Emri</th>
                                <th>Talep Oluşturuldu Mu?</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>



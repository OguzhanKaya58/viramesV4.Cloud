
@{
    ViewBag.Title = "workOrderOperationStep";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using viramesV4.Object.BusinessLogicLayer.Master
@model IList<WorkorderOperationStep>


<section class="content-header">
    <h1>
        İş Emri İşlem Adımları

    </h1>

    <div class="box-body ">
        <div class="btn-group">


            <button type="button" class="btn btn-primary ">
                <span class=" fa fa-file"></span> Yeni
            </button>

        </div>
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-edit"></span> Düzenle
        </button>

        <button type="button" class="btn btn-primary">
            <span class=" fa fa-eye"></span> Görüntüle
        </button>

        <button type="button" class="btn btn-primary ">
            <span class=" fa fa-flag"></span> İş Emri İşlem Adım Durumu
        </button>
        
        <button type="button" class="btn btn-primary">
            <span class=" fa fa-share-square"></span> Aktarım
        </button>
    </div>


    <ol class="breadcrumb">
        <li class="active">İş Emri İşlem Adımları </li>
    </ol>



</section>

<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">



            <!-- /.box -->

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title"></h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>İş Emri</th>
                                <th>İşlem Adımı</th>
                                <th>Malzeme Kodu ve Tanımı</th>
                                <th>Tamamlama</th>
                                <th>İş Emri İşlem Adım Durumu</th>
                                <th>İşlem Süresi</th>
                                <th>Hedeflenen Başlangıç Tarihi</th>
                                <th>Hedeflenen Bitiş Tarihi</th>
                                <th>Planlanan Başlangıç Tarihi</th>
                                <th>Planlanan Bitiş Tarihi</th>
                                <th>Gerçekleşen Başlangıç Tarihi</th>
                                <th>Gerçekleşen Bitiş Tarihi</th>
                                <th>Örtüşme Tarihi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (WorkorderOperationStep operationStep in Model)
                            {

                            <tr>
                                <td>@operationStep.Workorder</td>
                                <td>@operationStep.OperationStep</td>
                                <td>@operationStep.Workorder.Material</td>
                                <td>%@operationStep.Completion</td>
                                <td>@operationStep.WorkOrderOperationStepStatus</td>
                                <td>@operationStep.ProcessTime</td>
                                <td>@operationStep.TargetStartDate</td>
                                <td>@operationStep.TargetEndDate</td>
                                <td>@operationStep.PlannedStartDate</td>
                                <td>@operationStep.PlannedEndDate</td>
                                <td>@operationStep.ActualStartDate</td>
                                <td>@operationStep.ActualEndDate</td>
                                <td>@operationStep.OverlapDate</td>
                            </tr>

                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <th>İş Emri</th>
                                <th>İşlem Adımı</th>
                                <th>Malzeme Kodu ve Tanımı</th>
                                <th>Tamamlama</th>
                                <th>İş Emri İşlem Adım Durumu</th>
                                <th>İşlem Süresi</th>
                                <th>Hedeflenen Başlangıç Tarihi</th>
                                <th>Hedeflenen Bitiş Tarihi</th>
                                <th>Planlanan Başlangıç Tarihi</th>
                                <th>Planlanan Bitiş Tarihi</th>
                                <th>Gerçekleşen Başlangıç Tarihi</th>
                                <th>Gerçekleşen Bitiş Tarihi</th>
                                <th>Örtüşme Tarihi</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>







